value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"content-wrapper\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                    <div class=\"content-wrap card\">\n                        <div mbr-form class=\"mbr-form\">\n                            <form action=\"wit.piechocki@gmail.com\" name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\" data-verified=\"wit.piechocki@gmail.com\">\n                                <div class=\"row\">\n                                    <div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Dziękujemy za kontakt!</div>\n                                    <div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">\n                                        Oops...! some\n                                        problem!\n                                    </div>\n                                </div>\n                                <div class=\"dragArea row\">\n                                    <div class=\"col-lg-12 col-md-12 col-sm-12\">\n                                        <h3 mbr-theme-style=\"display-2\" class=\"mbr-fonts-style mbr-section-title\" data-app-selector=\".mbr-section-title, .mbr-section-btn\">\n                                            <b>Napisz do nas</b></h3>\n                                    </div>\n                                    <div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3 mb-3 mb-3 mb-3\" data-for=\"name\">\n                                        <input type=\"text\" name=\"name\" placeholder=\"Imię i Nazwisko\" data-form-field=\"name\" mbr-theme-style=\"display-4\" class=\"form-control\" value>\n                                    </div>\n                                    <div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3 mb-3 mb-3\" data-for=\"email\" style>\n                                        <input type=\"email\" name=\"email\" placeholder=\"Adres e-mail\" data-form-field=\"email\" mbr-theme-style=\"display-4\" class=\"form-control\" value>\n                                    </div>\n                                    <div data-for=\"text\" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n                                        <input type=\"text\" name=\"text\" placeholder=\"Telefon\" data-form-field=\"text\" mbr-theme-style=\"display-7\" class=\"form-control\" value>\n                                    </div>\n                                    <div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3 mb-3\" data-for=\"textarea\">\n                                <textarea name=\"textarea\" placeholder=\"Wiadomość\" data-form-field=\"textarea\" mbr-theme-style=\"display-4\" class=\"form-control\"></textarea>\n                                    </div>\n                                    <div mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"col mbr-section-btn\"><a type=\"submit\" class=\"btn btn-secondary\" data-app-placeholder=\"Type Text\">\n                                            <span class=\"mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn\"></span>\n                                            Wyślij wiadomość</a></div>\n                                </div>\n                            </form>\n                        </div>\n                    </div>\n                    <div class=\"image-wrapper\">\n                        <img src=\"@PROJECT_PATH@/assets/images/gem-1677x1677.jpg\" alt=\"shop\">\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
